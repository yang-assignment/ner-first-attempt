   "source": [
    "from pathlib import Path\n",
    "import re\n",
    "\n",
    "file_path = Path(\"g.txt\")\n",
    "raw_text = file_path.read_text(encoding='utf-8').strip()\n",
    "raw_text = re.sub(r'\\n\\n', \"\\n\", raw_text)\n",
    "\n",
    "with open('a.txt', 'w') as f:\n",
    "    f.write(raw_text)"
